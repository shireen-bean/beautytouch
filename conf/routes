# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /admin                      controllers.Application.admin()
GET     /login                      controllers.Application.login()
GET     /product                    controllers.Product.product()
GET     /productJson                controllers.Product.productJson(sku)
POST    /postProductImage           controllers.Product.postProductImage()
POST    /postProduct		        controllers.Product.postProduct()
GET     /productList                controllers.ProductList.productList()
GET     /productListJson            controllers.ProductList.productListJson()
GET     /logout                     controllers.Application.logout()
GET     /machineList                controllers.MachineList.machineList()
GET     /machineListJson            controllers.MachineList.machineListJson()
GET     /machine                    controllers.Machine.machine()
GET     /machineJson                controllers.Machine.machineJson(id)
GET     /checkoutProductSelect      controllers.Checkout.productSelect()
GET     /thankYou                   controllers.Checkout.thankYou(productId)
GET     /pay	                    controllers.Checkout.pay(productId)
GET     /contactus	                controllers.Landing.contactUs()
GET     /terms		                controllers.Landing.terms()
POST    /postMachine		        controllers.Machine.postMachine()
POST    /loginVending		        controllers.Application.loginVending()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
